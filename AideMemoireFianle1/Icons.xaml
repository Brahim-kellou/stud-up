                                    </Canvas>
                                </Canvas>
                            </Viewbox>
                            <TextBlock Text="MENU" FontWeight="Bold" VerticalAlignment="Center" Margin="15 0 0 0" Foreground="White"/>
                        </StackPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

